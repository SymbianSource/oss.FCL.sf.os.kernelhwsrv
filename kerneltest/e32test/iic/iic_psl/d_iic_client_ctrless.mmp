#include "kernel/kern_ext.mmh"

target         iic_client_ctrless.ldd
targettype     ldd

// Select the mode to build
// For Master-Slave mode, uncomment both of the two lines that follow
macro MASTER_MODE
macro SLAVE_MODE

macro IIC_SIMULATED_PSL
macro STANDALONE_CHANNEL

sourcepath     .
source         iic_client.cpp
OS_LAYER_SYSTEMINCLUDE_SYMBIAN

// un-comment the following if debug printing is required
//macro LOG_CLIENT

library spi_ctrless.lib
library i2c_ctrless.lib

uid			0x100000af 0x10286b4d
VENDORID 0x70000001

capability		all
epocallowdlldata

start wins
win32_headers
end

SMPSAFE


